          "lines": [
            {
              "type": "chord",
              "content": "Bm"
            },
            {
              "type": "lyric",
              "content": "¡Ay amor mío!"
            }
          ]
        },
        {
          "type": "voice",
          "name": "1º VOZ PATO",
          "color": "pato",
          "lines": [
            {
              "type": "chord",
              "content": "C#m"
            },
            {
              "type": "lyric",
              "content": "que terriblemente absurdo es estar vivo"
            }
          ]
        },
        {
          "type": "chords",
          "content": ["E", "A", "A4"]
        },
        {
          "type": "voice",
          "name": "2º VOZ ALE",
          "color": "ale",
