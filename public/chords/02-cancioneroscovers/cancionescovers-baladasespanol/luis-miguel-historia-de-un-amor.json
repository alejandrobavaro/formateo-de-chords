    {
      "type": "divider"
    },
    {
      "type": "section",
      "name": "ESTROFA 1",
      "lines": [
        {
          "type": "voice",
          "name": "1º VOZ ALE",
          "color": "ale",
          "lines": [
            {"type": "chord", "content": "E7"},
            {"type": "lyric", "content": "Ya no estás más a mi lado"},
            {"type": "chord", "content": "Dm"},
            {"type": "lyric", "content": "corazón"},
            {"type": "chord", "content": "E7"},
            {"type": "lyric", "content": "En el alma solo tengo"},
            {"type": "chord", "content": "Am"},
            {"type": "lyric", "content": "soledad"}
          ]
        },
        {
          "type": "voice",
          "name": "1º VOZ PATO",
          "color": "pato",
          "lines": [
            {"type": "chord", "content": "G"},
            {"type": "lyric", "content": "Y si ya no puedo verte"},
            {"type": "chord", "content": "F"},
            {"type": "lyric", "content": "Porque Dios me hizo quererte"},
            {"type": "chord", "content": "Dm"},
            {"type": "lyric", "content": "Para hacerme sufrir más"}
          ]
        }
      ]
    },
    {
      "type": "divider"
    },
    {
      "type": "section",
      "name": "PRE-ESTRIBILLO",
      "lines": [
        {
          "type": "voice",
          "name": "1º VOZ ALE",
          "color": "ale",
          "lines": [
            {"type": "chord", "content": "Dm"},
            {"type": "lyric", "content": "Siempre fuiste la razón"},
            {"type": "chord", "content": "G"},
            {"type": "lyric", "content": "de mi existir"},
            {"type": "chord", "content": "A7"},
            {"type": "lyric", "content": "Adorarte para mí fue religión"}
          ]
        },
        {
          "type": "voice",
          "name": "1º VOZ PATO",
          "color": "pato",
          "lines": [
            {"type": "chord", "content": "Dm"},
            {"type": "lyric", "content": "Y en tus besos yo encontraba"},
            {"type": "chord", "content": "C"},
            {"type": "lyric", "content": "El calor que me brindaba"},
            {"type": "chord", "content": "E"},
            {"type": "lyric", "content": "El amor, y la pasión"}
          ]
        }
      ]
    },
    {
      "type": "divider"
    },
    {
      "type": "section",
      "name": "ESTRIBILLO 1",
      "lines": [
        {
          "type": "voice",
          "name": "1º VOZ ALE",
          "color": "ale",
          "lines": [
            {"type": "chord", "content": "Dm"},
            {"type": "lyric", "content": "Es la historia de un amor"},
            {"type": "chord", "content": "G"},
            {"type": "lyric", "content": "Como no hay otro igual"}
          ]
        },
        {
          "type": "voice",
          "name": "2º VOZ PATO",
          "color": "pato",
          "lines": [
            {"type": "chord", "content": "A7"},
            {"type": "lyric", "content": ""},
            {"type": "chord", "content": "Dm"},
            {"type": "lyric", "content": "Que me hizo comprender"}
          ]
        },
        {
          "type": "voice",
          "name": "1º VOZ ALE",
          "color": "ale",
          "lines": [
            {"type": "chord", "content": "Dm"},
            {"type": "lyric", "content": "Todo el bien, todo el mal"},
            {"type": "chord", "content": "C"},
            {"type": "lyric", "content": "Que le dio luz a mi vida"},
            {"type": "chord", "content": "Dm"},
            {"type": "lyric", "content": "Apagándola después"}
          ]
        },
        {
          "type": "voice",
          "name": "2º VOZ PATO",
          "color": "pato",
          "lines": [
            {"type": "chord", "content": "G"},
            {"type": "lyric", "content": "Ay qué vida tan oscura"},
            {"type": "chord", "content": "A7"},
            {"type": "lyric", "content": "Sin tu amor no viviré"}
          ]
        }
