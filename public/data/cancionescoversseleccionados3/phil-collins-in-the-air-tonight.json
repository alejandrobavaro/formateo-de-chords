            {
              "type": "chords",
              "content": ["C"]
            },
            {
              "type": "lyric",
              "content": ""
            },
            {
              "type": "chords",
              "content": ["A#"]
            },
            {
              "type": "lyric",
              "content": "But I don't know if you know who I am"
