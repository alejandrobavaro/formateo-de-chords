{
  "id": "franco-de-vita-te-amo",
  "title": "Te Amo",
  "artist": "Franco De Vita",
  "originalKey": "Bm7",
  "tempo": "Moderate (Balada romántica)",
  "timeSignature": "4/4",
  "content": [
    {
      "type": "section",
      "name": "PRIMERA ESTROFA",
      "lines": [
        {
          "type": "chord",
          "content": "Bm7"
        },
        {
          "type": "lyric",
          "content": "Así nos hubieran visto,"
        },
        {
          "type": "chord",
          "content": "E"
        },
        {
          "type": "lyric",
          "content": "estábamos ahí sentados"
        },
        {
          "type": "chord",
          "content": "D"
        },
        {
          "type": "lyric",
          "content": "frente a frente."
        },
        {
          "type": "chord",
          "content": "C#m"
        },
        {
          "type": "lyric",
          "content": "No podía faltarnos la luna,"
        },
        {
          "type": "chord",
          "content": "F#m"
        },
        {
          "type": "lyric",
          "content": "y hablábamos de todo un poco,"
        },
        {
          "type": "chord",
          "content": "Bm7"
        },
        {
          "type": "lyric",
          "content": "y todo nos causaba risa"
        },
        {
          "type": "chord",
          "content": "E"
        },
        {
          "type": "lyric",
          "content": "como dos tontos."
        },
        {
          "type": "chord",
          "content": "D"
        },
        {
          "type": "lyric",
          "content": "Y yo que no veía la hora"
        },
        {
          "type": "chord",
          "content": "A"
        },
        {
          "type": "lyric",
          "content": "de tenerte en mis brazos"
        },
        {
          "type": "chord",
          "content": "E"
        },
        {
          "type": "lyric",
          "content": "y poderte decir..."
        }
      ]
    },
    {
      "type": "divider"
    },
    {
      "type": "section",
