          "lines": [
            {
              "type": "chord",
              "content": "A"
            },
            {
              "type": "lyric",
              "content": "Te amo"
            }
          ]
        },
        {
          "type": "voice",
          "name": "1º VOZ PATO",
          "color": "pato",
          "lines": [
            {
              "type": "chord",
              "content": "D"
            },
            {
              "type": "lyric",
              "content": "desde el primer momento en que te vi"
            }
          ]
        },
        {
          "type": "voice",
          "name": "2º VOZ ALE",
          "color": "ale",
          "lines": [
            {
              "type": "chord",
              "content": "D"
            },
            {
              "type": "lyric",
              "content": "y hace tiempo te buscaba"
            }
          ]
        },
        {
          "type": "voice",
