        "type": "voice",
        "name": "1º VOZ ALE",
        "color": "ale",
        "lines": [
          {
            "type": "chord",
            "content": "D"
          },
          {
            "type": "lyric",
            "content": "Dentro de tus ojos veo un lago"
          },
          {
            "type": "chord",
            "content": "G6"
          },
          {
            "type": "lyric",
            "content": "Donde un hada se desnuda"
          },
          {
            "type": "chord",
            "content": "D"
          },
          {
            "type": "lyric",
            "content": "Para que la adore el Sol"
          },
          {
            "type": "chord",
            "content": "G9"
          }
        ]
      },
      {
        "type": "divider"
      },
      {
        "type": "voice",
        "name": "1º VOZ PATO",
        "color": "pato",
        "lines": [
          {
            "type": "chord",
            "content": "D"
          },
          {
            "type": "lyric",
            "content": "La melancolía de la tarde"
          },
          {
            "type": "chord",
            "content": "G6"
          },
          {
            "type": "lyric",
            "content": "Me ha ganado el corazón"
          },
          {
            "type": "chord",
            "content": "D"
          },
          {
            "type": "lyric",
            "content": "Y se nubla de dudas"
          },
          {
            "type": "chord",
            "content": "G9"
          }
        ]
      },
      {
        "type": "divider"
      },
      {
        "type": "voice",
        "name": "1º VOZ ALE",
        "color": "ale",
        "lines": [
          {
            "type": "chord",
            "content": "D"
          },
          {
            "type": "lyric",
            "content": "Son esos momentos"
          },
          {
            "type": "chord",
            "content": "G6"
          },
          {
            "type": "lyric",
            "content": "En que uno se pone a reflexionar"
          },
          {
            "type": "chord",
            "content": "D"
          },
          {
            "type": "lyric",
            "content": "Y alumbra una tormenta"
          },
          {
            "type": "chord",
            "content": "G9"
          }
        ]
      },
      {
        "type": "divider"
      },
      {
        "type": "voice",
        "name": "1º VOZ PATO",
        "color": "pato",
        "lines": [
          {
            "type": "chord",
            "content": "D"
          },
          {
            "type": "lyric",
            "content": "Todo es tan tranquilo"
          },
          {
            "type": "chord",
            "content": "G6"
          },
          {
            "type": "lyric",
            "content": "Que el silencio anuncia el ruido"
          },
          {
            "type": "chord",
            "content": "D"
          },
          {
            "type": "lyric",
            "content": "De la calma que antecede al huracán"
          },
          {
            "type": "chord",
            "content": "G9"
          }
        ]
      },
      {
        "type": "divider"
      },
      {
        "type": "section",
        "name": "ESTRIBILLO",
