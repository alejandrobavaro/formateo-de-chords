          {
            "type": "voice",
            "name": "1º VOZ ALE",
            "color": "ale",
            "lines": [
              {
                "type": "chord",
                "content": "D"
              },
              {
                "type": "lyric",
                "content": "De repente no puedo respirar"
              }
            ]
          },
          {
            "type": "voice",
            "name": "2º VOZ PATO",
            "color": "pato",
            "lines": [
              {
                "type": "chord",
                "content": "Bm"
              },
              {
                "type": "lyric",
                "content": "Necesito un poco de libertad"
              }
            ]
          },
          {
            "type": "chords",
            "content": ["G"]
          },
          {
            "type": "lyric",
            "content": "Que te alejes por un tiempo de mi lado"
          },
          {
            "type": "chords",
            "content": ["A"]
          },
          {
            "type": "lyric",
            "content": "Que me dejes en paz"
          },
          {
            "type": "voice",
            "name": "1º VOZ ALE",
