          }
        ]
      },
      {
        "type": "divider"
      },
      {
        "type": "section",
        "name": "ESTROFA 1",
        "lines": [
          {
            "type": "voice",
            "name": "1º VOZ ALE",
            "color": "ale",
            "lines": [
              {
                "type": "chord",
                "content": "G"
              },
              {
                "type": "lyric",
                "content": "Quédate cada noche y sueñame en la luna,"
              },
              {
                "type": "chord",
                "content": "Em"
              },
              {
                "type": "lyric",
                "content": ""
              }
            ]
          },
          {
            "type": "voice",
            "name": "1º VOZ PATO",
            "color": "pato",
            "lines": [
              {
                "type": "chord",
                "content": "C"
              },
              {
                "type": "lyric",
                "content": "ámame por las calles perdidas de mi vida,"
              },
              {
                "type": "chord",
                "content": "D7"
              },
              {
                "type": "lyric",
                "content": ""
              }
            ]
          },
          {
            "type": "voice",
            "name": "1º VOZ ALE",
            "color": "ale",
            "lines": [
              {
                "type": "chord",
                "content": "G"
              },
              {
                "type": "lyric",
                "content": "bésame poco a poco, no sea que me muera,"
              },
              {
                "type": "chord",
                "content": "Em"
              },
              {
                "type": "lyric",
                "content": ""
              }
            ]
          },
          {
            "type": "voice",
            "name": "1º VOZ PATO",
            "color": "pato",
            "lines": [
              {
                "type": "chord",
                "content": "C"
              },
              {
                "type": "lyric",
                "content": "vive con este loco perdido que te quiere."
              },
              {
                "type": "chord",
                "content": "D7"
              },
              {
                "type": "lyric",
                "content": ""
              }
            ]
          }
        ]
      },
      {
        "type": "divider"
      },
      {
        "type": "section",
        "name": "CORO",
        "lines": [
          {
            "type": "chords",
            "content": ["G", "Em"]
          },
          {
            "type": "lyric",
            "content": "Si me falta tu presencia,"
          },
          {
            "type": "chords",
            "content": ["C"]
          },
          {
            "type": "lyric",
            "content": "yo me pierdo buscándote,"
          },
          {
            "type": "chords",
            "content": ["D7"]
          },
          {
            "type": "lyric",
            "content": "cierro mi vida, cierro para mí"
          },
          {
            "type": "chords",
            "content": ["G", "Em"]
          },
          {
            "type": "lyric",
            "content": "el libro blanco de mi vida,"
          },
          {
            "type": "chords",
            "content": ["C", "D7"]
          },
          {
            "type": "lyric",
            "content": "el libro blanco de mi vida,"
          },
          {
            "type": "chords",
            "content": ["G"]
          },
          {
            "type": "lyric",
            "content": "perdido sin ti,"
          },
          {
            "type": "chords",
            "content": ["Em"]
          },
          {
            "type": "lyric",
            "content": "sin ti."
          },
          {
            "type": "chords",
            "content": ["C", "D7"]
          },
          {
            "type": "lyric",
            "content": "I love you, I need you,"
          },
          {
            "type": "chords",
            "content": ["G", "Em"]
          },
          {
            "type": "lyric",
            "content": "estoy perdido sin ti."
          },
          {
            "type": "chords",
            "content": ["C"]
          },
          {
            "type": "lyric",
            "content": "No te vayas,"
          },
          {
            "type": "chords",
            "content": ["D7", "G"]
          },
          {
            "type": "lyric",
            "content": "espérate, amor, quédate, amor, quédate."
          }
        ]
      },
      {
        "type": "divider"
      },
      {
        "type": "section",
        "name": "ESTROFA 2",
        "lines": [
          {
            "type": "voice",
            "name": "1º VOZ ALE",
            "color": "ale",
            "lines": [
              {
                "type": "chord",
                "content": "G"
              },
              {
                "type": "lyric",
                "content": "Guárdate en mi memoria y escóndete en mis brazos,"
              },
              {
                "type": "chord",
                "content": "Em"
              },
              {
                "type": "lyric",
                "content": ""
              }
            ]
          },
          {
            "type": "voice",
            "name": "1º VOZ PATO",
            "color": "pato",
            "lines": [
              {
                "type": "chord",
                "content": "C"
              },
              {
                "type": "lyric",
                "content": "métete por los poros abiertos de mi vida,"
              },
              {
                "type": "chord",
                "content": "D7"
              },
              {
                "type": "lyric",
                "content": ""
              }
            ]
          },
          {
            "type": "voice",
            "name": "1º VOZ ALE",
            "color": "ale",
            "lines": [
              {
                "type": "chord",
                "content": "G"
              },
              {
                "type": "lyric",
                "content": "mírame desde cerca y mátame si quieres,"
              },
              {
                "type": "chord",
                "content": "Em"
              },
              {
                "type": "lyric",
                "content": ""
              }
            ]
          },
          {
            "type": "voice",
            "name": "1º VOZ PATO",
            "color": "pato",
            "lines": [
              {
                "type": "chord",
                "content": "C"
              },
              {
                "type": "lyric",
                "content": "pero no me abandones al viento de mi vía."
              },
              {
                "type": "chord",
                "content": "D7"
              },
              {
                "type": "lyric",
                "content": ""
              }
            ]
          }
        ]
      }
