          { "type": "chord", "content": "Dm" },
          { "type": "lyric", "content": "Aquí va mi confesión," },
          { "type": "chord", "content": "C" },
          { "type": "lyric", "content": "antes de ti no fui un santo" },
          { "type": "chord", "content": "A#" },
          { "type": "lyric", "content": "He pecado como no, pero eso es cosa del pasado" },
          { "type": "chord", "content": "Gm" },
          { "type": "lyric", "content": "Desde que llegaste tú" },
          { "type": "chord", "content": "F" },
          { "type": "lyric", "content": "lanzaste al aire la moneda" },
          { "type": "chord", "content": "Gm" },
          { "type": "lyric", "content": "Fuera cara o fuera cruz," },
          { "type": "chord", "content": "F" },
          { "type": "lyric", "content": "ganabas como quieras" },
          { "type": "chord", "content": "C" }
