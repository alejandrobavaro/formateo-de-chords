        "lines": [
          {
            "type": "voice",
            "name": "1º VOZ ALE",
            "color": "ale",
            "lines": [
              {"type": "chord", "content": "G"},
              {"type": "lyric", "content": "Y pensar que una vez"},
              {"type": "chord", "content": "D/F#"},
              {"type": "lyric", "content": "me dijiste terminemos todoo,"},
              {"type": "chord", "content": "Em"},
              {"type": "lyric", "content": "mi vida es un desastre"},
              {"type": "chord", "content": "Bm"},
              {"type": "lyric", "content": "y no te quiero en él,"},
              {"type": "chord", "content": "Am"},
              {"type": "lyric", "content": "pero al fin te seguí"},
              {"type": "chord", "content": "D"},
              {"type": "lyric", "content": "por un laberinto de espejos rotos,"},
              {"type": "chord", "content": "Am"},
              {"type": "lyric", "content": "y aparecí en un barrio,"},
              {"type": "chord", "content": "D"},
              {"type": "lyric", "content": "del que no puedo salir."}
            ]
          }
        ]
      },
