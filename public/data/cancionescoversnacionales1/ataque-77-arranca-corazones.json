        "name": "VERSO 1",
        "lines": [
          {
            "type": "voice",
            "name": "1º VOZ ALE",
            "color": "ale",
            "lines": [
              {"type": "chord", "content": "G"},
              {"type": "lyric", "content": "Evitar, resistir,"},
              {"type": "chord", "content": "D/F#"},
              {"type": "lyric", "content": "tu hechizo de suave adicción,"},
              {"type": "chord", "content": "Em"},
              {"type": "lyric", "content": "como si fuera fácil"},
              {"type": "chord", "content": "Bm"},
              {"type": "lyric", "content": "dominar mi sentir."},
              {"type": "chord", "content": "Am"},
              {"type": "lyric", "content": "Y saber que te vas,"},
              {"type": "chord", "content": "D"},
              {"type": "lyric", "content": "y saber que la abstinencia me puede,"},
              {"type": "chord", "content": "Am"},
              {"type": "lyric", "content": "todo se vuelve oscuro,"},
              {"type": "chord", "content": "D"},
              {"type": "lyric", "content": "y solo puedo decir."}
            ]
          }
        ]
      },
      {
        "type": "section",
        "name": "ESTRIBILLO",
        "lines": [
          {
            "type": "voice",
            "name": "1º VOZ ALE",
            "color": "ale",
            "lines": [
              {"type": "chord", "content": "G"},
              {"type": "lyric", "content": "Más, dame un poco más,"},
              {"type": "chord", "content": "Em"},
              {"type": "lyric", "content": "quiero intoxicarme en vos,"},
              {"type": "chord", "content": "Bm"},
              {"type": "lyric", "content": "arranca corazones."},
              {"type": "chord", "content": "D"},
              {"type": "lyric", "content": "Hoy antes del final,"},
              {"type": "chord", "content": "C"},
              {"type": "lyric", "content": "quiero intoxicarme en vooos,"},
              {"type": "chord", "content": "G"},
              {"type": "lyric", "content": "arranca corazones,"},
              {"type": "chord", "content": "Em"},
              {"type": "lyric", "content": "dame tu droga."}
            ]
          }
        ]
      },
      {
        "type": "section",
